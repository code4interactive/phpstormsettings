  </template>
  <template name="panels" value="&lt;div class=&quot;panel panel-default&quot;&gt;&#10;  &lt;div class=&quot;panel-heading&quot;&gt;&lt;/div&gt;&#10;  &lt;div class=&quot;panel-body&quot;&gt;&#10;    &#10;    &#10;  &lt;/div&gt;&#10;&lt;/div&gt;" description="Bootstrap Panel" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
