<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_DOC_COMMENTS" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="ERROR_STRIPE_MARK_MIN_HEIGHT" value="3" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="PowerlineSymbols" />
    <option name="LINE_SPACING" value="2.0" />
  </component>
  <component name="EditorSettings">
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern icon="question" pattern="\bdeprecated\b.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>